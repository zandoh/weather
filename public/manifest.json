{
  "manifest_version": 2,
  "name": "Tempest",
  "author": "Zac Clark",
  "version": "0.1.0",
  "description": "Replace new tab page with customizable weather application.",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
